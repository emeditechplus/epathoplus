                <div class="row col-xs-12">
                    <div style="margin-top:10px;"></div>
                    <div class="col-xs-1">
                        Tot&nbsp;Case:
                    </div>
                    <div class="col-xs-1">
                        @Html.TextBoxFor(model => model.TotalCase, new { @class = "form-control", @onkeypress = "return true;", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1">
                        Remarks:
                    </div>
                    <div class="col-xs-2">
                        @Html.TextAreaFor(model => model.Remarks, new { @class = "form-control", @rows = 4, @onkeypress = "return true;", style = "width: 250px;height: 95px;" })
                    </div>
                    <div class="col-xs-1">
                        &nbsp;
                    </div>
                    <div class="col-xs-6">
                        <input type="submit" id="btnsave" value="Save" class="btn btn-primary" />
                        <input type="submit" id="btnclose" value="Close" class="btn btn-danger" />
                        <input type="submit" id="btnApprove" value="Approve" class="btn btn-success" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

