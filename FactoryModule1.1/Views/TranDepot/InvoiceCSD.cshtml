                    </div>
                    <div class="col-xs-1" style="display:none;">
                        Disc&nbsp;Amt.:
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        @Html.TextBoxFor(model => model.TotalDiscountAmt, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        Free&nbsp;Amt.:
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        @Html.TextBoxFor(model => model.TotalFreeAmt, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                </div>

                <div class="row col-xs-12">
                    <div style="margin-top:10px;"></div>
                    <div class="col-xs-1" style="display:none;">
                        SS(%):
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        @Html.TextBoxFor(model => model.SSMargin, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        SS&nbsp;Amt.:
                    </div>
                    <div class="col-xs-1" style="display:none;">
                        @Html.TextBoxFor(model => model.SSMarginAmt, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1">
                        R/O :
                    </div>
                    <div class="col-xs-1">
                        @Html.TextBoxFor(model => model.RoundOff, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1">
                        Invoice&nbsp;Amt :
                    </div>
                    <div class="col-xs-1">
                        @Html.TextBoxFor(model => model.NetAmt, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1">
                        Gross&nbsp;Wght.:
                    </div>
                    <div class="col-xs-1">
                        @Html.TextBoxFor(model => model.TotalGrossWght, new { @class = "form-control", @onkeypress = "return true;", style = "width: 110px;height: 25px;" })
                    </div>
                    <div class="col-xs-1">
                        Tot&nbsp;PCS:
                    </div>
                    <div class="col-xs-1">
                        @Html.TextBoxFor(model => model.TotalPcs, new { @class = "form-control", @disabled = "disabled", style = "width: 110px;height: 25px;" })
